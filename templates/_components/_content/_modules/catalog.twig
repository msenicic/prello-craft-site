{% set heading    = module.heading %}
{% set text       = module.text %}
{% set topTitle   = module.topTitle %}
{% set superTable = module.slider.all() %}
{% set moduleId   = module.moduleId %}

<section id="{{ moduleId ?: 'module-' ~ moduleIndex }}" class="catalog">
    <div class="container">
        <div class="main-text">
            <div class="top-title">
                <p>{{ topTitle }}</p>
            </div>
            <div class="title">
                <p>{{ heading }}</p>
            </div>
            <div class="text">
                <p>{{ text }}</p>
            </div>
        </div>
        <div class="slider">
            <div class="inner">
                <div class="slider-wrapper">
                    <div class="swiper catalog-swiper">
                        <div class="swiper-wrapper">
                            {% for slider in superTable %}
                                {% set phoneImage = slider.phoneImage.one() %}
                                <div class="swiper-slide">
                                    <div class="phone">
                                        <img src="{{ siteUrl }}assets/images/Vector.png" alt="" class="shape">
                                        <img src="{{ phoneImage.url }}" alt="" class="p">
                                    </div>
                                </div>
                            {% endfor %}
                        </div>
                        <div class="swiper-pagination"></div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>